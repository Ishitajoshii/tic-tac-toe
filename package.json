{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "A simple Tic Tac Toe game",
  "main": "js/script.js",
  "scripts": {
    "test": "jest",
    "start": "npm run build && live-server --port=3000",
    "build": "echo 'No build step needed for this project'",
    "lint": "eslint js/*.js",
    "format": "prettier --write ."
  },
  "keywords": [
    "tic-tac-toe",
    "game",
    "javascript",
    "html",
    "css"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "live-server": "^1.2.2",
    "prettier": "^3.1.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}